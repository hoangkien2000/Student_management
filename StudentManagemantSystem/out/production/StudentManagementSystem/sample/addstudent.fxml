<AnchorPane onKeyPressed="#Key_Handle_Back" prefHeight="700.0" prefWidth="608.0" stylesheets="@../../css/style.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AddStudentController">
